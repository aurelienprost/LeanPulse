<?xml version="1.0"?>
<fop version="1.0">
  <base>.</base>
  <renderers>
    <renderer mime="application/pdf">
      <fonts>
        <auto-detect/>
      </fonts>
    </renderer>
  </renderers>
  <fonts>
  	<substitutions>
  		<substitution>
			<from font-family="Tms Rmn"/>
			<to font-family="Times"/>
		</substitution>
	</substitutions>
  </fonts>
</fop>

